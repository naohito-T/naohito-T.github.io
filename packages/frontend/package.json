{
  "name": "frontend",
  "description": "Package core logic",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^16.17.0",
    "npm": "^8.15.0"
  },
  "scripts": {
    "dev": "next dev -p 2000",
    "build": "next build && next export",
    "start": "serve out",
    "deploy": "gh-pages -d out",
    "lint": "run-p t:check l:write p:write",
    "t:check": "tsc --noEmit",
    "l:check": "next lint",
    "l:write": "next lint --fix",
    "p:check": "prettier -c './**/*.{js,jsx,ts,tsx,json}'",
    "p:write": "prettier -w './**/*.{js,jsx,ts,tsx,json}'",
    "s:check": "stylelint '**/*.scss'",
    "s:write": "stylelint --fix '**/*.scss'",
    "jest": "jest -c ./tests/jest.config.ts",
    "jest:u": "jest -u -c ./tests/jest.config.ts",
    "jest:w": "jest --watch -c ./tests/jest.config.ts",
    "jest:c": "jest -c ./tests/jest.config.ts --coverage --silent"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@react-p5/core": "^1.1.0",
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "gsap": "^3.11.3",
    "locomotive-scroll": "^4.1.4",
    "microcms-js-sdk": "^2.3.2",
    "next": "12.3.1",
    "p5": "^1.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-locomotive-scroll": "^0.2.2",
    "react-p5": "^1.3.33",
    "styled-components": "^5.3.6",
    "styled-media-query": "^2.1.2",
    "three": "^0.147.0",
    "typescript": "4.9.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/styled-components": "^5.1.26",
    "@types/three": "^0.146.0",
    "gh-pages": "^4.0.0",
    "jest-environment-jsdom": "^29.3.1",
    "sass": "^1.56.2",
    "serve": "^14.1.2"
  }
}
